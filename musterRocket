host:${host}
database:${database}
user:${user}
password:${password}

#Put here the informationen of your Bot token and the channel/group/chat you want to to display the messages
[Bot Settings]
token:${token}

#Main Mesage Kanal
chat_id:-100-100${chatid_rocket}
